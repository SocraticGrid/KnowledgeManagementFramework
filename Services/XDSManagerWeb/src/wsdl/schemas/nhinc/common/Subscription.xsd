    <xsd:complexType name="UnsubscribeType">
        <xsd:sequence>
            <xsd:element name="subscriptionReference" type="tns:SubscriptionReferenceType">
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="Unsubscribe" type="tns:UnsubscribeType">
    </xsd:element>
