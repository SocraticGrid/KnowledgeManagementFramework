<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="patientDataFact">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="factType" type="xs:string"/>
				<xs:element name="facts" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="patientId" type="xs:int"/>
							<xs:element name="contactId" type="xs:string"/>
							<xs:element name="name" type="xs:string"/>
							<xs:element name="lastName" type="xs:string"/>
							<xs:element name="firstName" type="xs:string"/>
							<xs:element name="middleName"  type="xs:string"/>
							<xs:element name="age" type="xs:byte"/>
							<xs:element name="gender" type="xs:string"/>
							<xs:element name="address" type="xs:string"/>
							<xs:element name="city" type="xs:string"/>
							<xs:element name="state" type="xs:string"/>
							<xs:element name="dateOfBirth" type="xs:date"/>
							<xs:element name="gender" type="xs:string"/>
							<xs:element name="ssn" type="xs:string"/>
							<xs:element name="medicalRecordNo" type="xs:string"/>
							<xs:element name="primaryDoctor" type="xs:string"/>
							<xs:element name="patientStatus" type="xs:string"/>
							<xs:element name="emergencyContact" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
